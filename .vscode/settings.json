{
  "[markdown]": {
    "editor.rulers": [
      80,
      120
    ]
  },
  "[yaml]": {
    "editor.rulers": [
      80,
      120
    ]
  },
  "files.associations": {
    ".coveragerc": "ini",
    "**.service": "ini",
    ".env.example": "properties"
  },
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./tests",
    "-p",
    "test_*.py"
  ],
  "python.formatting.autopep8Args": [
    "--max-line-length",
    "80",
    "--in-place",
    "--aggressive",
    "--aggressive",
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true
}
